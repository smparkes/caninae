!!!5
%html
  %head
    %title= content_for?(:title) ? content_for(:title) : "Tollerdata"
    %meta{charset:"UTF-8"}
    = stylesheet_link_tag    "tollerdata/tollerdata", :media => "all"
    = javascript_include_tag "tollerdata"
    = csrf_meta_tags
  %body
    = yield
    %hr/
    %font{:size => "1"}
      Use of this site is subject to terms and conditions as expressed on the
      = succeed "." do
        = link_to "home page", tollerdata_path
%p/
%form#form2{:action => "https://www.paypal.com/cgi-bin/webscr", :method => "POST", :name => "form2"}
  %p
    %input{:name => "cmd", :type => "hidden", :value => "_s-xclick"}/
    %input{:name => "encrypted", :type => "hidden", :value => "-----BEGIN PKCS7----------END PKCS7-----"}/
    %input{:alt => "PayPal - The safer, easier way to pay online!", :border => "0", :name => "submit", :src => "https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif", :type => "image"}/
    %img{:alt => "", :border => "0", :height => "1", :src => "https://www.paypalobjects.com/en_US/i/scr/pixel.gif", :width => "1"}/
