= form_for @succeed do |f|
  - if @succeed.errors.any?
    #error_explanation
      %h2= "#{pluralize(@succeed.errors.count, "error")} prohibited this succeed from being saved:"
      %ul
        - @succeed.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :first
    = f.number_field :first
  .field
    = f.label :second
    = f.number_field :second
  .actions
    = f.submit 'Save'
