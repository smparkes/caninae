= form_for @dominate do |f|
  - if @dominate.errors.any?
    #error_explanation
      %h2= "#{pluralize(@dominate.errors.count, "error")} prohibited this dominate from being saved:"
      %ul
        - @dominate.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :dominator_id
    = f.number_field :dominator_id
  .field
    = f.label :dominated_id
    = f.number_field :dominated_id
  .actions
    = f.submit 'Save'
