= form_for @kennel_litter do |f|
  - if @kennel_litter.errors.any?
    #error_explanation
      %h2= "#{pluralize(@kennel_litter.errors.count, "error")} prohibited this kennel_litter from being saved:"
      %ul
        - @kennel_litter.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :kennel_id
    = f.number_field :kennel_id
  .field
    = f.label :litter_id
    = f.number_field :litter_id
  .actions
    = f.submit 'Save'
